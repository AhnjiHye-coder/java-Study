package operator;

public class Ex06 {
	public static void main(String[] args) {
		// 대입연산
		// 우변의 값을 좌변의 공간에 복사하여 저장한다. 기존 값을 덮어쓴다
		int n1 = 10;
		int n2 = 20;
		
		n1 = n2;
		// n2의 값을 n1변수에 복사하여 대입한다. 기존의 10은 사라지고 20으로 바뀐다
		System.out.println("n1 : " + n1);
		System.out.println("n2 : " + n2);
	}
}
